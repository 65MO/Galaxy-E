planemo tool_init --force \
	--id 'gdalinfo' \
	--name 'Display information about a GDAL supported dataset' \
	--requirement gdal@2.1.0 \
	--example_command 'gdalinfo dataset/stere.tif > dataset/stere_info' \
	--example_input dataset/stere.tif \
	--example_output dataset/stere_info \
	--test_case \
	--cite_url 'http://www.gdal.org/' \
	--help_from_command 'gdalinfo'
